    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "zod": "^3.21.4"
  },
